{
  "name": "hyperliquid-mcp-servers",
  "version": "1.0.0",
  "description": "Collection of Hyperliquid MCP servers in JavaScript/TypeScript",
  "scripts": {
    "install-all": "npm run install-mektigboy && npm run install-balthazar && npm run install-6rz6",
    "install-mektigboy": "cd mektigboy && npm install",
    "install-balthazar": "cd tradingbalthazar && npm install",
    "install-6rz6": "cd 6rz6 && npm install",
    "start-mektigboy": "cd mektigboy && npm start",
    "start-balthazar": "cd tradingbalthazar && npm start",
    "start-6rz6": "cd 6rz6 && npm start",
    "test": "npm run test-mektigboy && npm run test-balthazar",
    "test-mektigboy": "cd mektigboy && npm test",
    "test-balthazar": "cd tradingbalthazar && npm test"
  },
  "keywords": [
    "hyperliquid",
    "mcp",
    "trading",
    "defi"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
